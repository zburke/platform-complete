{
  "name": "@folio/platform-complete",
  "version": "1.1.0-SNAPSHOT",
  "license": "Apache-2.0",
  "scripts": {
    "build": "export NODE_OPTIONS=\"--max-old-space-size=4096 $NODE_OPTIONS\"; stripes build stripes.config.js",
    "stripes": "stripes",
    "start": "stripes serve stripes.config.js",
    "build-module-descriptors": "stripes mod descriptor stripes.config.js --output ./ModuleDescriptors",
    "local": "f=stripes.config.js; test -f $f.local && f=$f.local; echo Using config $f; stripes serve $f",
    "test": "echo 'No unit tests implemented'",
    "test-int": "stripes test nightmare stripes.config.js",
    "test-regression": "stripes test nightmare stripes.config.js --run WD/platform-core/checkout/users/inventory/requests/circulation/tenant-settings"
  },
  "dependencies": {
    "@folio/developer": "folio-org/ui-developer#boom",
    "@folio/stripes": "^7.0.0",
    "moment": "~2.29.0",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-intl": "5.21.0",
    "react-query": "^3.13.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-titled": "^1.0.1",
    "redux": "^4.0.5",
    "rxjs": "^6.6.7",
    "swr": "^0.4.2",

    "@folio/bugsnag-logger": "zburke/ui-bugsnag-logger",
    "@folio/rollbar-logger": "zburke/ui-rollbar-logger",
    "@folio/sentry-logger": "zburke/ui-sentry-logger"

  },
  "devDependencies": {
    "@folio/stripes-cli": "^2.0.0",
    "eslint": "^6.2.1",
    "lodash": "^4.17.5"
  },
  "resolutions": {
    "@folio/stripes-cli": "^2.4.0",
    "@rehooks/local-storage": "2.4.0",
    "colors": "1.4.0",
    "final-form": "^4.20.4",
    "minimist": "^1.2.3",
    "moment": "~2.29.0",
    "redux-form": "^8.0.0", 
    "@cerner/duplicate-package-checker-webpack-plugin": "2.1.0"
  }
}
